                @Html.DisplayNameFor(model => model.NguoiThucHien)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LoaiGiaoDich)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DiaChiVi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SoLuong)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NgayGiaoDich)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PhiGiaoDich)
